   </define>
   <define name="tei_att.styleDef.attribute.scheme">
      <optional>
         <attribute name="scheme">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">identifie la langue employée pour décrire le rendu</a:documentation>
            <choice>
               <value>css</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">langage CSS (Cascading Stylesheet )</a:documentation>
               <value>xslfo</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">Langage XSL (Extensible Stylesheet )Formatting Objects</a:documentation>
               <value>free</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">description en texte libre non structuré.</a:documentation>
               <value>other</value>
               <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">langue de description de l'interprétation définie par l'utilisateur</a:documentation>
            </choice>
         </attribute>
      </optional>
   </define>
   <define name="tei_att.styleDef.attribute.schemeVersion">
      <optional>
         <attribute name="schemeVersion">
            <a:documentation xmlns:a="http://relaxng.org/ns/compatibility/annotations/1.0">supplies a version number for the style language provided in scheme.</a:documentation>
            <data type="token">
               <param name="pattern">[\d]+[a-z]*[\d]*(\.[\d]+[a-z]*[\d]*){0,3}</param>
            </data>
         </attribute>
      </optional>
   </define>
   <pattern xmlns="http://purl.oclc.org/dsdl/schematron"
            id="myTEI-att.styleDef-schemeVersion-schemeVersionRequiresScheme-constraint-rule-7">
      <sch:rule xmlns:rng="http://relaxng.org/ns/structure/1.0"
                xmlns:sch="http://purl.oclc.org/dsdl/schematron"
                xmlns="http://www.tei-c.org/ns/1.0"
                context="tei:*[@schemeVersion]">
            <sch:assert test="@scheme and not(@scheme = 'free')">
              @schemeVersion can only be used if @scheme is specified.
            </sch:assert>
          </sch:rule>
   </pattern>
